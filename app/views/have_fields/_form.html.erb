<%= semantic_form_for @have_field do |f| %>
  <%= f.inputs do %>
    <%= f.input :id %>
    <%= f.input :name %>
    <%= f.input :field_name %>
    <%= f.input :field_type %>
    <%= f.input :field_ui_type %>
    <%= f.input :field_ds %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
