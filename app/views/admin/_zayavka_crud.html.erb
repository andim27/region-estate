        getHaveTabIndex:function () {
            var tab = $('#tbs-haves').tabs('getSelected');
            var index = $('#tbs-haves').tabs('getTabIndex',tab);
            return (index);
        },
        initHaveEvents:function (index) {
            //----event set-----------------------
           console.log("initHaveEvents index="+index)
